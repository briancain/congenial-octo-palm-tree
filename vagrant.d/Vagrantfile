# -*- mode: ruby -*-
# vi: set ft=ruby :

Vagrant.configure("2") do |config|
  config.vm.provider :vmware_desktop do |v|
    v.cpus = 2
  end

  config.vm.provision "THE END", after: :all, inline:<<-SHELL
  echo 'the end!!111'
  SHELL

  config.vm.provider :virtualbox do |vb|
    vb.linked_clone = true
  end
end
